{
  "name": "node-ts",
  "version": "0.0.0",
  "author": "dayblox",
  "description": "Node.js & TypeScript starter",
  "engines": {
    "node": ">=18.11",
    "pnpm": ">=7.27"
  },
  "scripts": {
    "dev": "pnpm /dev:.*/",
    "dev:build": "esbuild dev.ts ./src --bundle --outdir=dist --packages=external --platform=node --sourcemap --watch",
    "dev:run": "node --inspect --no-warnings --loader ./dist/dev.js --watch dist/src",
    "build": "esbuild index=./src --bundle --minify --outdir=dist --packages=external --platform=node",
    "start": "node dist"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.8.6",
    "@prisma/client": "^4.13.0",
    "@upstash/ratelimit": "^0.4.2",
    "@upstash/redis": "^1.20.4",
    "cloudinary": "^1.36.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.17",
    "express": "^4.18.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  }
}
